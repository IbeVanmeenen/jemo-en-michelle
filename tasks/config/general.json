{
  "root": {
    "npm": "./",
    "dev": "./dev",
    "dist": "./dist"
  },
  "javascript": {
    "dev": "js",
    "dist": "assets/js",
    "extensions": [".json", ".js"]
  },
  "styles": {
    "dev": "scss/**/*",
    "dist": "assets/css",
    "extensions": ".scss"
  },
  "images": {
    "dev": "img/**/*",
    "dist": "assets/img",
    "extensions": ".{jpg,png,svg,gif}"
  },
  "audio": {
    "dev": "sound/*",
    "dist": "assets/sound",
    "extensions": ".mp3"
  },
  "html": {
    "dev": "html/**/*",
    "dist": "./",
    "extensions": ".html"
  }
}
